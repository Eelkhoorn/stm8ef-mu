#!/usr/bin/awk -f

$1==":" {fl=-1;print;next}
fl==-1 && $1~/^..$/ {fl=0;$1="\t"$1" c,\t\t";print "  [ "$0;next}
fl==-1 && $1~/^....$/ {fl=0;$1="\t"$1" ,\t\t";print "  [ "$0;next}
$1~/^......$/ {$1="\t" substr($1,1,2)" c, "substr($1,3,4)" , "} 
$1~/^....$/ {$1="\t"$1" ,\t\t"}
$1~/^..$/ {$1="\t"$1" c,\t\t"}
$2==";" {$2=" ] ;";print $1$2"\t"$3" "$4" "$5" "$6" "$7" "$8 ; next}
fl==0 {print} 
