-- STM8 eForth ]CB  copy carry to addr.bit in-line              TG9541, MM-170927
-- ------------------------------------------------------------------------------
.ifndef (]CB)

: (]CB) ;

meta
  : ]CB ( [CY] addr bit -- )
    -- compile BCCM addr,#bit
    $90 \m c, \f 2* $11 or \m c, \m , \m ]
  ;

.then