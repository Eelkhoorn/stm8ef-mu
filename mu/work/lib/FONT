-- 475 bytes
__meta
eeprom
hex
here constant FONT   -- 5x8    
  00 c, 00 c, 00 c, 00 c, 00 c, -- space
  00 c, 00 c, 2F c, 00 c, 00 c, -- !
  00 c, 07 c, 00 c, 07 c, 00 c, -- "
