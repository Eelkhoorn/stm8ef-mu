-- STM8 eForth ]BC  copy addr.bit to carry in-line              TG9541, MM-170927
-- ------------------------------------------------------------------------------
.ifndef (]BC)

: (]BC)

meta

  : ]BC ( addr bit -- [CY] )
    -- compile BTJT addr,#bit,0 - copies addr.bit to carry
    $72 \m c, 2* \m c, \m , 0 \m , \m ]
  ;

.then
